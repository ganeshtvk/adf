{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorytvk"
		},
		"ls_BlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_BlobStorage'"
		},
		"ls_FileServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_FileServer'"
		},
		"ls_SQLdatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_SQLdatabase'"
		},
		"ls_adlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsgen2'"
		},
		"ls_FileServer_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "F:\\SelfIntegrated"
		},
		"ls_FileServer_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "GANESH"
		},
		"ls_adlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storeadlsgen2.dfs.core.windows.net"
		},
		"ls_http_github_step2c_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorytvk_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_BlobStorage_connectionString": {
						"value": "[parameters('ls_BlobStorage_connectionString')]"
					},
					"ls_SQLdatabase_connectionString": {
						"value": "[parameters('ls_SQLdatabase_connectionString')]"
					},
					"ls_adlsgen2_accountKey": {
						"value": "[parameters('ls_adlsgen2_accountKey')]"
					},
					"ls_adlsgen2_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsgen2_properties_typeProperties_url')]"
					},
					"ls_http_github_step2c_properties_typeProperties_url": {
						"value": "[parameters('ls_http_github_step2c_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "datafactorytvk_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_FileServer_password": {
						"value": "[parameters('ls_FileServer_password')]"
					},
					"ls_FileServer_properties_typeProperties_host": {
						"value": "[parameters('ls_FileServer_properties_typeProperties_host')]"
					},
					"ls_FileServer_properties_typeProperties_userId": {
						"value": "[parameters('ls_FileServer_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactorytvk_ArmTemplate_0')]"
			]
		},
		{
			"name": "datafactorytvk_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactorytvk_ArmTemplate_1')]"
			]
		}
	]
}